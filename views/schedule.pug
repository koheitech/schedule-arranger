extends layout

block content
  h4 #{schedule.scheduleName}
  p(style="white-space:pre;") #{schedule.memo}
  p Created by: #{schedule.user.username}
  h3 Availabilities
  table
    tr
      th Schedule
      each user in users
        th #{user.username}
    each candidate in candidates
      tr
        th #{candidate.candidateName}
        each user in users
          td 
            button Absence
